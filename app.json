{
  "expo": {
    "name": "Mustang Match",
    "slug": "datingapp",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*",
      "node_modules/@expo/vector-icons/fonts/*",
      "node_modules/react-native-vector-icons/Fonts/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.michaelsaunichev.datingapp",
      "buildNumber": "11",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSExceptionDomains": {
            "ec2-3-135-241-67.us-east-2.compute.amazonaws.com": {
              "NSIncludesSubdomains": true,
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSExceptionMinimumTLSVersion": "TLSv1.0",
              "NSExceptionRequiresForwardSecrecy": false,
              "NSRequiresCertificateTransparency": false
            },
            "3.135.241.67": {
              "NSIncludesSubdomains": true,
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSExceptionMinimumTLSVersion": "TLSv1.0",
              "NSExceptionRequiresForwardSecrecy": false,
              "NSRequiresCertificateTransparency": false
            }
          }
        },
        "NSPhotoLibraryUsageDescription": "This app requires access to your photo library to allow you to upload profile pictures. You can change this permission in the general settings at any time.",
        "NSPhotoLibraryAddUsageDescription": "This app requires access to your photo library to save profile pictures. You can change this permission in the general settings at any time."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.michaelsaunichev.datingapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-build-properties"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b12fc3b5-cefe-4bdf-812c-1bf740f362f3"
      }
    }
  }
}